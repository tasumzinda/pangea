<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fillViewport="true">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <include layout="@layout/toolbar" />

        <Button
            android:id="@+id/btn_submit"
            style="?android:attr/buttonBarButtonStyle"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:background="@drawable/selectable_item_background"
            android:text="Submit"
            android:textColor="@color/white" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:text="Supplimentary Indicator Form"
            android:textStyle="bold" />

        <TextView
            style="@style/TextViewInputStyle"
            android:text="Select Facility"></TextView>

        <Spinner
            android:id="@+id/facility"
            style="@style/SpinnerStyle" />
        <EditText
                android:id="@+id/facility_label"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"/>

        <TextView
            style="@style/TextViewInputStyle"
            android:text="Select Period"></TextView>

        <Spinner
            android:id="@+id/period"
            style="@style/SpinnerStyle" />

        <TextView
            style="@style/TextViewInputStyle"
            android:text="Date Completed" />

        <EditText
            android:id="@+id/dateCreated"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:hint="Date Completed" />

        <TextView
            style="@style/TextViewInputStyle"
            android:text="Name" />

        <EditText
            android:id="@+id/name"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:hint="Name" />

        <TableLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TableRow
                android:layout_width="wrap_content"
            android:layout_height="wrap_content"
                android:padding="5dip">

                <TextView
                    style="@style/TextViewInputStyle"
                    android:layout_height="wrap_content"
                    android:layout_width="200dp"
                    android:paddingBottom="20dp"
                    android:text="1. Estimated Facility Catchment Population" />

                <EditText
                    android:id="@+id/estimatedCatchmentPopulation"
                    android:layout_width="120dp"
                    android:layout_height="wrap_content"
                    android:inputType="number" />

            </TableRow>

            <TableRow
                android:layout_width="wrap_content"
            android:layout_height="wrap_content"
                android:padding="5dip">

                <TextView
                    style="@style/TextViewInputStyle"
                    android:layout_height="wrap_content"
                    android:layout_width="200dp"
                    android:paddingBottom="20dp"
                    android:text="2. Number Of Pre-ART Patients" />

                <EditText
                    android:id="@+id/numberOfPreARTPatients"
                    android:layout_width="120dp"
                    android:layout_height="wrap_content"
                    android:inputType="number" />

            </TableRow>

            <TableRow
                android:layout_width="wrap_content"
            android:layout_height="wrap_content"
                android:padding="5dip">

                <TextView
                    style="@style/TextViewInputStyle"
                    android:layout_height="wrap_content"
                    android:layout_width="200dp"
                    android:paddingBottom="20dp"
                    android:text="3. No. of CLFs deployed" />

                <EditText
                    android:id="@+id/numberOfCLFsDeployed"
                    android:layout_width="120dp"
                    android:layout_height="wrap_content"
                    android:inputType="number" />

            </TableRow>

            <TableRow
                android:layout_width="wrap_content"
            android:layout_height="wrap_content"
                android:padding="5dip">

                <TextView
                    style="@style/TextViewInputStyle"
                    android:layout_height="wrap_content"
                    android:layout_width="200dp"
                    android:paddingBottom="20dp"
                    android:text="4. Date CLFs Deployed" />

                <EditText
                    android:id="@+id/dateCLFsDeployed"
                    android:layout_width="120dp"
                    android:layout_height="wrap_content"
                    android:inputType="number" />

            </TableRow>

            <TableRow
                android:layout_width="wrap_content"
            android:layout_height="wrap_content"
                android:padding="5dip">

                <TextView
                    style="@style/TextViewInputStyle"
                    android:layout_height="wrap_content"
                    android:layout_width="200dp"
                    android:paddingBottom="20dp"
                    android:text="5. Number of Active CARGs" />

                <EditText
                    android:id="@+id/numberOfActiveCARGs"
                    android:layout_width="120dp"
                    android:layout_height="wrap_content"
                    android:inputType="number" />

            </TableRow>

            <TableRow
                android:layout_width="wrap_content"
            android:layout_height="wrap_content"
                android:padding="5dip">

                <TextView
                    style="@style/TextViewInputStyle"
                    android:layout_height="wrap_content"
                    android:layout_width="200dp"
                    android:paddingBottom="20dp"
                    android:text="6. Number of Active CARG members" />

                <EditText
                    android:id="@+id/numberOfActiveCARGMembers"
                    android:layout_width="120dp"
                    android:layout_height="wrap_content"
                    android:inputType="number" />

            </TableRow>

            <TableRow
                android:layout_width="wrap_content"
            android:layout_height="wrap_content"
                android:padding="5dip">

                <TextView
                    style="@style/TextViewInputStyle"
                    android:layout_height="wrap_content"
                    android:layout_width="200dp"
                    android:paddingBottom="20dp"
                    android:text="7. Number of CARGs formed this month" />

                <EditText
                    android:id="@+id/numberOfCARGsFormedThisMonth"
                    android:layout_width="120dp"
                    android:layout_height="wrap_content"
                    android:inputType="number" />
            </TableRow>


            <TableRow
                android:layout_width="wrap_content"
            android:layout_height="wrap_content"
                android:padding="5dip">

                <TextView
                    style="@style/TextViewInputStyle"
                    android:layout_height="wrap_content"
                    android:layout_width="200dp"
                    android:paddingBottom="20dp"
                    android:text="8. Number of CARGS formed to date" />

                <EditText
                    android:id="@+id/numberOfCARGsFormedToDate"
                    android:layout_width="120dp"
                    android:layout_height="wrap_content"
                    android:inputType="number" />

            </TableRow>

            <TableRow
                android:layout_width="wrap_content"
            android:layout_height="wrap_content"
                android:padding="5dip">

                <TextView
                    style="@style/TextViewInputStyle"
                    android:layout_height="wrap_content"
                    android:layout_width="200dp"
                    android:paddingBottom="20dp"
