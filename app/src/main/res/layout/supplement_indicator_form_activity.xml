            </TableRow>

            <TableRow
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="5dip">

                <TextView
                        style="@style/TextViewInputStyle"
                        android:layout_width="200dp"
                        android:paddingBottom="20dp"
                        android:layout_height="wrap_content"
                        android:text="42. Number of clients with documented completed referral cycle from community to facility" />

                <EditText
                        android:id="@+id/numberOfClientsWithDocumentedCompletedReferralCycleFromCommunityToFacility"
                        android:layout_width="120dp"
                        android:layout_height="wrap_content"
                        android:inputType="number" />
            </TableRow>

            <TableRow
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="5dip">

                <TextView
                        style="@style/TextViewInputStyle"
                        android:layout_width="200dp"
                        android:paddingBottom="20dp"
                        android:layout_height="wrap_content"
                        android:text="43. Number of clients who defaulted ART" />

                <EditText
                        android:id="@+id/numberOfClientsWhoDefaultedART"
                        android:layout_width="120dp"
                        android:layout_height="wrap_content"
                        android:inputType="number" />
            </TableRow>

            <TableRow
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="5dip">

                <TextView
                        style="@style/TextViewInputStyle"
                        android:layout_width="200dp"
                        android:paddingBottom="20dp"
                        android:layout_height="wrap_content"
                        android:text="44. Number of defaulted clients who were followed and have a documented outcome" />

                <EditText
                        android:id="@+id/numberOfClientsWhoWereFollowedAndHaveADocumentedOutcome"
                        android:layout_width="120dp"
                        android:layout_height="wrap_content"
                        android:inputType="number" />
            </TableRow>

            <TableRow
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="5dip">

                <TextView
                        style="@style/TextViewInputStyle"
                        android:layout_width="200dp"
                        android:paddingBottom="20dp"
                        android:layout_height="wrap_content"
                        android:text="45. Number of EID results received and successfully issued to caregivers" />

                <EditText
                        android:id="@+id/numberOfEIDResultsReceivedAndSuccessfullyIssuedToCaregivers"
                        android:layout_width="120dp"
                        android:layout_height="wrap_content"
                        android:inputType="number" />
            </TableRow>

            <TableRow
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="5dip">

                <TextView
                        style="@style/TextViewInputStyle"
                        android:layout_width="200dp"
                        android:paddingBottom="20dp"
                        android:layout_height="wrap_content"
                        android:text="46. Number of HIV infected infants tracked and linked back to care" />

                <EditText
                        android:id="@+id/numberOfHIVInfectedInfantsTrackedAndLinkedBackToCare"
                        android:layout_width="120dp"
                        android:layout_height="wrap_content"
                        android:inputType="number" />
            </TableRow>

            <TableRow
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="5dip">

                <TextView
                        style="@style/TextViewInputStyle"
                        android:layout_width="200dp"
                        android:paddingBottom="20dp"
                        android:layout_height="wrap_content"
                        android:text="47. Number village Health Workers Working with I-ITECH" />

                <EditText
                        android:id="@+id/numberOfVillageHealthWorkersWorkingWithITECH"
                        android:layout_width="120dp"
                        android:layout_height="wrap_content"
                        android:inputType="number" />
            </TableRow>

        </TableLayout>

        <Button
            android:id="@+id/btn_save"
            style="?android:attr/buttonBarButtonStyle"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:background="@drawable/selectable_item_background"
            android:text="Save"
            android:textColor="@color/white" />

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="?android:attr/dividerHorizontal" />

        <Button
            android:id="@+id/btn_completed"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:background="@null"
            android:drawablePadding="16dp"
            android:drawableTop="@mipmap/ic_check"
            android:text="Submitted"
            android:textAllCaps="false"
            android:textColor="@android:color/black"
            android:textSize="24sp" />

    </LinearLayout>
</ScrollView>
